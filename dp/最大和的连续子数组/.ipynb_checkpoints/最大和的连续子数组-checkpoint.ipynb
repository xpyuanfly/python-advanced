   "source": [
    "class Solution:\n",
    "    def maxSubArray(self, nums: List[int]) -> int:\n",
    "        f=[0 for i in range(len(nums))]\n",
    "        f[0]=nums[0]\n",
    "        for i in range(1,len(nums)):\n",
    "            c1=nums[i]\n",
    "            c2=f[i-1]+nums[i]\n",
    "            f[i]=max(c1,c2)\n",
    "        return max(f)"
   ]
  }
